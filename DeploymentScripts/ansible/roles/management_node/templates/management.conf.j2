<?xml version="1.0" encoding="utf-8"?>
<NodeConfiguration>
	<Account>
		<Username>themanagement</Username>
		<Domain>{{domain_name}}</Domain>
		<Password>{{monitron_management_password}}</Password>
	</Account>
	<MessangerClient>
		<DllPath>.</DllPath>
		<DllName>Monitron.Clients.XMPP.dll</DllName>
		<Type>Monitron.Clients.XMPP.XMPPMessengerClient</Type>
	</MessangerClient>
	<Plugin>
	 	<DllPath>.</DllPath>
		<DllName>Monitron.Plugins.Management.dll</DllName>
		<Type>Monitron.Plugins.Management.ManagementPlugin</Type>
	</Plugin>
	<DataStore>
	 	<DllPath>.</DllPath>
		<DllName>Monitron.PluginDataStore.Local.dll</DllName>
		<Type>Monitron.PluginDataStore.Local.LocalPluginDataStore</Type>
		<Location>/etc/monitron/datastore.json</Location>
	</DataStore>
 </NodeConfiguration>
